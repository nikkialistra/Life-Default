    <Style src="project://database/Assets/Resources/UI/Styles/GameLook/Components/ColonistEquipmentTab.uss?fileID=7433441132597879392&amp;guid=baeee912499c1bf4793f296b27f0c8c0&amp;type=3#ColonistEquipmentTab" />
    <Style src="project://database/Assets/Resources/UI/Styles/GameLook/Components/ColonistPanel.uss?fileID=7433441132597879392&amp;guid=57c3dde8601441008a8e9f518deebeb1&amp;type=3#ColonistPanel" />
    <ui:VisualElement name="colonist-equipment-tab" picking-mode="Ignore" class="colonist-panel">
        <ui:VisualElement name="state-stats" class="stats bottom-indent">
            <ui:Label text="State" display-tooltip-when-elided="true" name="state-stats-header" class="label label-general left-aligned label-indented-big margin-bottom" />
            <ui:VisualElement name="state-stats-list">
