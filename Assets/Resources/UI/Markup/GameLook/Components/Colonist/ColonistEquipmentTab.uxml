                </ui:VisualElement>
                <ui:VisualElement name="comfortable-temperature" class="stat">
                    <ui:Label text="Comfortable Temperature:" display-tooltip-when-elided="true" name="comfortable-temperature__name" class="label left-aligned stat__name" />
                    <ui:Label text="100%" display-tooltip-when-elided="true" name="comfortable-temperature__value" class="label stat__value right-aligned" />
                </ui:VisualElement>
            </ui:VisualElement>
